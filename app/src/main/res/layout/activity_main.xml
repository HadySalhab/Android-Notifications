<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="16dp"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/edit_text_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Title"/>
    <EditText
        android:id="@+id/edit_text_message"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="Message"/>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send Big Text Style"
        android:onClick="sendOnChannel1"/>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send Inbox Style"
        android:onClick="sendOnChannel2"/>

    <Button
        android:onClick="sendOnChannel3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send  big Picture Style" />

    <Button
        android:onClick="sendOnChannel4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send  Media Style" />

    <Button
    android:onClick="sendOnChannel5"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:text="Send Messaging Style" />

    <Button
        android:onClick="sendOnChannel6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send progress bar Notification " />
    <Button
        android:onClick="sendOnChannel7"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Send Notification Groups " />


</LinearLayout>